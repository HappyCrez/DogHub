LOG_LEVEL=-1/0/1        # -1 quite, 0 info, 1 all

### Configurate a connection to database
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password
DB_NAME=doghub_db


### Email service settings for notifications
# If mail_service off, another settings isn't required
MAIL_SERVICE=ON/OFF

# email sender account
MAIL_HOST=user@gmail.com        # corporative email
MAIL_PASSWORD=app_password      # app pass is different from email pass

# File to send as message
# Path is releative from Server/DogHub/
# Can receive params in text will be replaced as $$
# To pass params see MailService/MailService.cs
MAIL_FILE_PATH=../../Assets/MailNotification.html

# Time in seconds between emails checks
MAIL_TIMEOUT=60

JWT_ISSUER=DogHubApi
JWT_AUDIENCE=DogHubClient
JWT_SECRET=super_secret_key_change_me_very_long_123456
ACCESS_TOKEN_LIFETIME_MINUTES=30